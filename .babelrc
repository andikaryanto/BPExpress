{
	"presets": [
		"@babel/preset-env",
		"@babel/preset-react",
		"@babel/preset-typescript"
	],
	"plugins": [
		[
			"@babel/transform-runtime"
		],
		[
			"@babel/plugin-proposal-class-properties"
		],
		//     ["transform-react-jsx", { "pragma": "h" }]
		[
			"@babel/plugin-transform-react-jsx",
			{
				"pragma": "h",
				"pragmaFrag": "Fragment",
			}
		]
	],
	"ignore": [
		"**/src/Write/*.log"
	]
}